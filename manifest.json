{
    "manifest_version": 3,
    "name": "Healthy Dev Habits",
    "version": "1.0",
    "description": "Healthy Dev Habits chrome extension",
    "permissions": [
        "activeTab", 
        "storage", 
        "alarms",
        "notifications",
        "background",
        "https://exercisedb.p.rapidapi.com/",
        "https://spoonacular.com/food-api/",
        "https://www.eventbriteapi.com/"
    ],
    "action": {
        "default_popup": "index.html"
      },
      "background": {
        "service_worker": "background.js"
      },
      "options_page": "settings.html"
}